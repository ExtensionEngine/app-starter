  "scripts": {
    "build:client": "rimraf dist/client && vue-cli-service build",
    "build:server": "rimraf dist/server && tsc",
    "dev:client": "vue-cli-service serve",
    "dev:server": "tsnd server/framework/start",
    "server:debug": "tsnd --inspect -- server/framework/start",
    "prestart": "npm run build:server",
    "start": "NODE_ENV=production node dist/server/framework/start",
    "clean": "del-cli dist/* !dist/.gitkeep",
    "lint": "eslint server",
    "lint:fix": "eslint --fix server",
    "user:add": "ts-node server/scripts/add-user",
    "db:migration:up": "npx mikro-orm migration:up",
    "db:migration:down": "npx mikro-orm migration:down",
    "db:dump": "npx mikro-orm schema:create --dump",
    "create:resource": "hygen generator resource",
    "create:controller": "hygen generator resource:controller",
    "create:repository": "hygen generator resource:.*repository*",
    "create:middleware": "hygen generator resource:middleware",
    "create:model": "hygen generator resource:model",
    "create:router": "hygen generator resource:index",
    "test": "echo \"Error: no test specified\" && exit 1",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "postshrinkwrap": "rewrite-lockfile package-lock.json"
  },
